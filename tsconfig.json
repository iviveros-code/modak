{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "react-native",
    "paths": {
      "@components": ["src/components"],
      "@config": ["src/config"],
      "@constants": ["src/constants"],
      "@navigation": ["src/navigation"],
      "@services": ["src/services"],
      "@types": ["src/types"],
      "@assets": ["./assets"],
      "@theme": ["src/theme"],
      "@redux/*": ["src/redux/*"],
      "@helpers": ["src/helpers"],
      "@screens": ["src/screens"],
      "@i18n": ["src/i18n"]
    }
  }
}
